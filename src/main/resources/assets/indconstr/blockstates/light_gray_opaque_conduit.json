{
    "multipart": [
        {   
        	"when": { "north": true },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_connection" }
        },
        {   
        	"when": { "north": false },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_side" }
        },
        {   
        	"when": { "east": true },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "east": false },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_side", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "south": true },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "south": false },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_side", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "west": true },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "west": false },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_side", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "up": true },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_vertical_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "up": false },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_top", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "down": true },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_vertical_connection", "uvlock": true, "x": 90 }
        },
        {   
        	"when": { "down": false },
            "apply": { "model": "indconstr:block/light_gray_opaque_conduit_top", "uvlock": true, "x": 90 }  
        }              
	]
}