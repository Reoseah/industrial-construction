{
    "multipart": [
        {   
        	"when": { "north": "none" },
            "apply": { "model": "indconstr:block/lime_conduit_side" }
        },
        {   
        	"when": { "east": "none" },
            "apply": { "model": "indconstr:block/lime_conduit_side", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "south": "none" },
            "apply": { "model": "indconstr:block/lime_conduit_side", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "west": "none" },
            "apply": { "model": "indconstr:block/lime_conduit_side", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "up": "none" },
            "apply": { "model": "indconstr:block/lime_conduit_top", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "down": "none" },
            "apply": { "model": "indconstr:block/lime_conduit_top", "uvlock": true, "x": 90 }  
        },
        
        {   
        	"when": { "north": "normal" },
            "apply": { "model": "indconstr:block/lime_conduit_connection" }
        },
        {   
        	"when": { "east": "normal" },
            "apply": { "model": "indconstr:block/lime_conduit_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "south": "normal" },
            "apply": { "model": "indconstr:block/lime_conduit_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "west": "normal" },
            "apply": { "model": "indconstr:block/lime_conduit_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "up": "normal" },
            "apply": { "model": "indconstr:block/lime_conduit_vertical_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "down": "normal" },
            "apply": { "model": "indconstr:block/lime_conduit_vertical_connection", "uvlock": true, "x": 90 }
        },
        
        {   
        	"when": { "north": "special" },
            "apply": { "model": "indconstr:block/opaque_conduit_connection" }
        },
        {   
        	"when": { "east": "special" },
            "apply": { "model": "indconstr:block/opaque_conduit_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "south": "special" },
            "apply": { "model": "indconstr:block/opaque_conduit_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "west": "special" },
            "apply": { "model": "indconstr:block/opaque_conduit_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "up": "special" },
            "apply": { "model": "indconstr:block/opaque_conduit_vertical_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "down": "special" },
            "apply": { "model": "indconstr:block/opaque_conduit_vertical_connection", "uvlock": true, "x": 90 }
        },

        {   
        	"when": { "north": "normal|special", "west": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint" }  
        },
        {   
        	"when": { "north": "normal|special", "east": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "y": 90 }  
        },
        {   
        	"when": { "south": "normal|special", "east": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "y": 180 }  
        },
        {   
        	"when": { "south": "normal|special", "west": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "y": 270 }  
        },
        
        {   
        	"when": { "west": "normal|special", "up": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 270 }  
        },
        {   
        	"when": { "north": "normal|special", "up": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 270, "y": 90 }  
        },
        {   
        	"when": { "east": "normal|special", "up": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 270, "y": 180 }  
        },
        {   
        	"when": { "south": "normal|special", "up": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 270, "y": 270 }  
        },
        
        {   
        	"when": { "west": "normal|special", "down": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 90 }  
        },
        {   
        	"when": { "north": "normal|special", "down": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 90, "y": 90 }  
        },
        {   
        	"when": { "east": "normal|special", "down": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 90, "y": 180 }  
        },
        {   
        	"when": { "south": "normal|special", "down": "normal|special" },
            "apply": { "model": "indconstr:block/conduit_joint", "uvlock": true, "x": 90, "y": 270 }  
        },
        
        
        {   
        	"when": { "direction": "north" },
            "apply": { "model": "indconstr:block/extractor_connection" }
        },
        {   
        	"when": { "direction": "east" },
            "apply": { "model": "indconstr:block/extractor_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "direction": "south" },
            "apply": { "model": "indconstr:block/extractor_connection", "uvlock": true, "y": 180 }
        },      
        {   
        	"when": { "direction": "west" },
            "apply": { "model": "indconstr:block/extractor_connection", "uvlock": true, "y": 270 }
        },        
        {   
        	"when": { "direction": "up" },
            "apply": { "model": "indconstr:block/extractor_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "direction": "down" },
            "apply": { "model": "indconstr:block/extractor_connection", "uvlock": true, "x": 90 }  
        }
	]
}