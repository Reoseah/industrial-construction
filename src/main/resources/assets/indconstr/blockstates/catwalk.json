{
    
    "multipart": [
        {   
            "apply": { "model": "indconstr:block/catwalk_floor_treadplate" }
        },
        {   
        	"when": { "north": true },
            "apply": { "model": "indconstr:block/catwalk_fence" }
        },
        {   
        	"when": { "east": true },
            "apply": { "model": "indconstr:block/catwalk_fence", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "south": true },
            "apply": { "model": "indconstr:block/catwalk_fence", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "west": true },
            "apply": { "model": "indconstr:block/catwalk_fence", "uvlock": true, "y": 270 }
        }
    ]
}