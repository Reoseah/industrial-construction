{   
    "multipart": [
        {   
        	"when": { "facing": "north", "half": "lower" },
            "apply": { "model": "icon:block/catwalk_stairs" }
        },
        {   
        	"when": { "facing": "east", "half": "lower" },
            "apply": { "model": "icon:block/catwalk_stairs", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "facing": "south", "half": "lower" },
            "apply": { "model": "icon:block/catwalk_stairs", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "facing": "west", "half": "lower" },
            "apply": { "model": "icon:block/catwalk_stairs", "uvlock": true, "y": 270 }
        },
        
        {   
        	"when": { "facing": "north", "half": "lower", "left": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "facing": "north", "half": "lower", "right": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail_right", "uvlock": true, "y": 90 }
        },
        
        {   
        	"when": { "facing": "east", "half": "lower", "left": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "facing": "east", "half": "lower", "right": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail_right", "uvlock": true, "y": 180 }
        },
        
        {   
        	"when": { "facing": "south", "half": "lower", "left": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "facing": "south", "half": "lower", "right": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail_right", "uvlock": true, "y": 270 }
        },
        
        {   
        	"when": { "facing": "west", "half": "lower", "left": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail", "uvlock": true }
        },
        {   
        	"when": { "facing": "west", "half": "lower", "right": true },
            "apply": { "model": "icon:block/catwalk_stairs_rail_right", "uvlock": true }
        }
    ]
}