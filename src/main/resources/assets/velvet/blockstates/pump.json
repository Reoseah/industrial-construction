{
    "multipart": [
        {   
        	"when": { "north": true },
            "apply": { "model": "velvet:block/conduit_connection" }
        },
        {   
        	"when": { "north": false },
            "apply": { "model": "velvet:block/pump_end" }
        },
        {   
        	"when": { "north": true, "direction": "north", "lit": false },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay" }
        },
        {   
        	"when": { "north": true, "direction": "north", "lit": true },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay_on" }
        },
        
        {   
        	"when": { "east": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "east": false },
            "apply": { "model": "velvet:block/pump_end", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "east": true, "direction": "east", "lit": false },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "east": true, "direction": "east", "lit": true },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay_on", "uvlock": true, "y": 90 }
        },
        
        
        {   
        	"when": { "south": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "south": false },
            "apply": { "model": "velvet:block/pump_end", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "south": true, "direction": "south", "lit": false },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "south": true, "direction": "south", "lit": true },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay_on", "uvlock": true, "y": 180 }
        },
        
        
        {   
        	"when": { "west": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "west": false },
            "apply": { "model": "velvet:block/pump_end", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "west": true, "direction": "west", "lit": false  },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "west": true, "direction": "west", "lit": false  },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay_on", "uvlock": true, "y": 270 }
        },
        
        
        {   
        	"when": { "up": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "up": false },
            "apply": { "model": "velvet:block/pump_end_top", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "up": true, "direction": "up", "lit": false },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "up": true, "direction": "up", "lit": true },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay_on", "uvlock": true, "x": 270 }
        },
        
        {   
        	"when": { "down": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "x": 90 }
        },
        {   
        	"when": { "down": false },
            "apply": { "model": "velvet:block/pump_end_top", "uvlock": true, "x": 90 }  
        },
        {   
        	"when": { "down": true, "direction": "down", "lit": false },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay", "uvlock": true, "x": 90 }  
        },
        {   
        	"when": { "down": true, "direction": "down", "lit": true },
            "apply": { "model": "velvet:block/powered_conduit_connection_overlay_on", "uvlock": true, "x": 90 }  
        }            
	]
}