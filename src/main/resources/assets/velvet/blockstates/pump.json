{
    "multipart": [
        {   
        	"when": { "north": true },
            "apply": { "model": "velvet:block/pump_connection" }
        },
        {   
        	"when": { "north": false },
            "apply": { "model": "velvet:block/pump_end" }
        },
        
        {   
        	"when": { "east": true },
            "apply": { "model": "velvet:block/pump_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "east": false },
            "apply": { "model": "velvet:block/pump_end", "uvlock": true, "y": 90 }
        },
        
        {   
        	"when": { "south": true },
            "apply": { "model": "velvet:block/pump_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "south": false },
            "apply": { "model": "velvet:block/pump_end", "uvlock": true, "y": 180 }
        },
        
        
        {   
        	"when": { "west": true },
            "apply": { "model": "velvet:block/pump_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "west": false },
            "apply": { "model": "velvet:block/pump_end", "uvlock": true, "y": 270 }
        },
        
        
        {   
        	"when": { "up": true },
            "apply": { "model": "velvet:block/pump_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "up": false },
            "apply": { "model": "velvet:block/pump_end_top", "uvlock": true, "x": 270 }
        },
        
        {   
        	"when": { "down": true },
            "apply": { "model": "velvet:block/pump_connection", "uvlock": true, "x": 90 }
        },
        {   
        	"when": { "down": false },
            "apply": { "model": "velvet:block/pump_end_top", "uvlock": true, "x": 90 }  
        }         
	]
}