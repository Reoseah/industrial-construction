{
    "multipart": [
        {   
        	"when": { "direction": "north" },
            "apply": { "model": "velvet:block/extractor_connection" }
        },
        {   
        	"when": { "direction": "east" },
            "apply": { "model": "velvet:block/extractor_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "direction": "south" },
            "apply": { "model": "velvet:block/extractor_connection", "uvlock": true, "y": 180 }
        },      
        {   
        	"when": { "direction": "west" },
            "apply": { "model": "velvet:block/extractor_connection", "uvlock": true, "y": 270 }
        },        
        {   
        	"when": { "direction": "up" },
            "apply": { "model": "velvet:block/extractor_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "direction": "down" },
            "apply": { "model": "velvet:block/extractor_connection", "uvlock": true, "x": 90 }  
        },
        
        {   
        	"when": { "output": "none|down|up|south|west|east" },
            "apply": { "model": "velvet:block/conduit_end" }
        },
        {   
        	"when": { "output": "none|down|up|north|south|west" },
            "apply": { "model": "velvet:block/conduit_end", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "output": "none|down|up|north|west|east" },
            "apply": { "model": "velvet:block/conduit_end", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "output": "none|down|up|north|south|east" },
            "apply": { "model": "velvet:block/conduit_end", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "output": "none|down|north|south|east|west" },
            "apply": { "model": "velvet:block/conduit_end_top", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "output": "none|up|north|south|east|west" },
            "apply": { "model": "velvet:block/conduit_end_top", "uvlock": true, "x": 90 }
        },
        
        {   
        	"when": { "output": "north" },
            "apply": { "model": "velvet:block/conduit_connection" }
        },
        {   
        	"when": { "output": "east" },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "output": "south" },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "output": "west" },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "output": "up" },
            "apply": { "model": "velvet:block/conduit_connection_y", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "output": "down" },
            "apply": { "model": "velvet:block/conduit_connection_y", "uvlock": true, "x": 90 }
        }
	]
}