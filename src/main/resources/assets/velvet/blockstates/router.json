{
    "multipart": [
        {   
        	"when": { "direction": "north" },
            "apply": { "model": "velvet:block/router_output" }
        },
        {   
        	"when": { "direction": "east" },
            "apply": { "model": "velvet:block/router_output", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "direction": "south" },
            "apply": { "model": "velvet:block/router_output", "uvlock": true, "y": 180 }
        },      
        {   
        	"when": { "direction": "west" },
            "apply": { "model": "velvet:block/router_output", "uvlock": true, "y": 270 }
        },        
        {   
        	"when": { "direction": "up" },
            "apply": { "model": "velvet:block/router_output", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "direction": "down" },
            "apply": { "model": "velvet:block/router_output", "uvlock": true, "x": 90 }  
        },
        
        {   
        	"when": { "direction": "up|down|south|west|east" },
            "apply": { "model": "velvet:block/router_input" }
        },
        {   
        	"when": { "direction": "up|down|south|north|west" },
            "apply": { "model": "velvet:block/router_input", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "direction": "up|down|north|west|east" },
            "apply": { "model": "velvet:block/router_input", "uvlock": true, "y": 180 }
        },      
        {   
        	"when": { "direction": "up|down|south|north|east" },
            "apply": { "model": "velvet:block/router_input", "uvlock": true, "y": 270 }
        },        
        {   
        	"when": { "direction": "down|south|north|west|east" },
            "apply": { "model": "velvet:block/router_input", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "direction": "up|south|north|west|east" },
            "apply": { "model": "velvet:block/router_input", "uvlock": true, "x": 90 }  
        },
        
        {   
        	"when": { "north": true },
            "apply": { "model": "velvet:block/conduit_connection" }
        },
        {   
        	"when": { "east": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "south": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "west": true },
            "apply": { "model": "velvet:block/conduit_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "up": true },
            "apply": { "model": "velvet:block/conduit_connection_y", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "down": true },
            "apply": { "model": "velvet:block/conduit_connection_y", "uvlock": true, "x": 90 }
        }
	]
}