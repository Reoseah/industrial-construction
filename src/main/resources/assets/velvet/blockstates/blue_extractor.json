        {   
        	"when": { "direction": "north" },
            "apply": { "model": "velvet:block/extractor_connection" }
        },
        {   
        	"when": { "direction": "east" },
            "apply": { "model": "velvet:block/extractor_connection", "uvlock": true, "y": 90 }
