{
    "multipart": [
        {   
        	"when": { "north": true },
            "apply": { "model": "velvet:block/blue_conduit_connection" }
        },
        {   
        	"when": { "north": false },
            "apply": { "model": "velvet:block/blue_conduit_end" }
        },
        {   
        	"when": { "east": true },
            "apply": { "model": "velvet:block/blue_conduit_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "east": false },
            "apply": { "model": "velvet:block/blue_conduit_end", "uvlock": true, "y": 90 }
        },
        {   
