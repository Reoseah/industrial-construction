{
    "multipart": [
        {   
        	"when": { "north": true },
            "apply": { "model": "iconstruct:block/gearbox_connection" }
        },
        {   
        	"when": { "north": false },
            "apply": { "model": "iconstruct:block/gearbox_side" }
        },
        {   
        	"when": { "east": true },
            "apply": { "model": "iconstruct:block/gearbox_connection", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "east": false },
            "apply": { "model": "iconstruct:block/gearbox_side", "uvlock": true, "y": 90 }
        },
        {   
        	"when": { "south": true },
            "apply": { "model": "iconstruct:block/gearbox_connection", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "south": false },
            "apply": { "model": "iconstruct:block/gearbox_side", "uvlock": true, "y": 180 }
        },
        {   
        	"when": { "west": true },
            "apply": { "model": "iconstruct:block/gearbox_connection", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "west": false },
            "apply": { "model": "iconstruct:block/gearbox_side", "uvlock": true, "y": 270 }
        },
        {   
        	"when": { "up": true },
            "apply": { "model": "iconstruct:block/gearbox_vertical_connection", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "up": false },
            "apply": { "model": "iconstruct:block/gearbox_top", "uvlock": true, "x": 270 }
        },
        {   
        	"when": { "down": true },
            "apply": { "model": "iconstruct:block/gearbox_vertical_connection", "uvlock": true, "x": 90 }
        },
        {   
        	"when": { "down": false },
            "apply": { "model": "iconstruct:block/gearbox_top", "uvlock": true, "x": 90 }  
        }
	]
}